{
  "name": "@vertexvis/rollup-plugin-vertexvis-copyright",
  "version": "0.1.0",
  "description": "Rollup plugin that prepends a Vertex copyright to JS bundles.",
  "author": "Dan Schultz (https://github.com/danschultz)",
  "license": "UNLICENSED",
  "main": "./dist/bundle.cjs.js",
  "module": "./dist/bundle.esm.js",
  "typings": "./dist/index.d.ts",
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "sideEffects": false,
  "files": [
    "dist/*",
    "!dist/**/__tests__"
  ],
  "scripts": {
    "clean": "rm -fr ./dist && mkdir ./dist",
    "format": "yarn lint --fix",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx --ignore-path ../../.gitignore .",
    "prebuild": "yarn clean",
    "build": "./node_modules/@vertexvis/build-tools/bin/build-web.sh",
    "test": "echo 'No unit tests defined'",
    "test:coverage": "echo 'No unit tests defined'"
  },
  "dependencies": {
    "rollup": "^1.19.4"
  },
  "devDependencies": {
    "@vertexvis/build-tools": "0.7.1",
    "@vertexvis/eslint-config-vertexvis-typescript": "0.2.0",
    "@vertexvis/typescript-config-vertexvis": "1.0.0"
  },
  "peerDependencies": {
    "eslint": "^6.1.0",
    "typescript": "^3.7.4"
  }
}
