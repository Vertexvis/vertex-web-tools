{
  "name": "@vertexvis/rollup-plugin-vertexvis-copyright",
  "version": "0.1.1",
  "description": "Rollup plugin that prepends a Vertex copyright to JS bundles.",
  "author": "Dan Schultz (https://github.com/danschultz)",
  "license": "MIT",
  "main": "./dist/bundle.cjs.js",
  "module": "./dist/bundle.esm.js",
  "typings": "./dist/index.d.ts",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "sideEffects": false,
  "files": [
    "dist/*",
    "!dist/**/__tests__"
  ],
  "scripts": {
    "clean": "rm -fr ./dist && mkdir ./dist",
    "format": "yarn lint --fix",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx --ignore-path ../../../.gitignore .",
    "prebuild": "yarn clean",
    "build": "./scripts/build-web.sh",
    "test": "echo 'No unit tests defined'",
    "test:coverage": "echo 'No unit tests defined'"
  },
  "dependencies": {
    "rollup": "^1.19.4"
  },
  "devDependencies": {
    "@vertexvis/eslint-config-vertexvis-typescript": "0.2.1",
    "@vertexvis/typescript-config-vertexvis": "1.0.1",
    "rollup-plugin-terser": "^5.1.1",
    "rollup-plugin-typescript2": "^0.22.1"
  },
  "peerDependencies": {
    "eslint": "^6.1.0",
    "typescript": "^3.7.4"
  }
}
